{
  "name": "vscode-phpintel",
  "version": "0.0.2",
  "description": "VSCode extension for phpintel",
  "repository": "https://github.com/john-nguyen09/vscode-phpintel",
  "main": "lib/extension.js",
  "publisher": "JohnNguyen",
  "engines": {
    "vscode": "^1.26.0"
  },
  "scripts": {
    "package": "npm run bundle && vsce package",
    "bundle": "rimraf lib && webpack",
    "build": "tsc",
    "update-vscode": "vscode-install",
    "postinstall": "vscode-install"
  },
  "author": "John Nguyen",
  "license": "MIT",
  "dependencies": {
    "mz": "^2.7.0",
    "request": "^2.88.0",
    "semver": "^6.3.0",
    "tar-stream": "^2.1.0",
    "unzipper": "^0.10.5",
    "vscode-languageclient": "^5.0.1"
  },
  "activationEvents": [
    "onLanguage:php",
    "workspaceContains:**/*.php"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "phpintel"
    }
  },
  "devDependencies": {
    "@types/mz": "0.0.32",
    "@types/node": "^10.7.1",
    "@types/request": "^2.48.3",
    "@types/semver": "^6.2.0",
    "@types/tar-stream": "^1.6.1",
    "@types/unzipper": "^0.10.1",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.0.1",
    "vscode": "^1.1.36",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
